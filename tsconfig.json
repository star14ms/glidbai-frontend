{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@nuxt/types",
      "@types/node",
      "@nuxtjs/auth-next"
    ]
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist",
    "node_modules_dev",
    "node_modules_prod",
    "pages-unused"
  ],
  "importHelpers": true,
  "vueCompilerOptions": {
    "target": 2.7,
    "extensions": [
      ".vue"
    ],
    "plugins": [
      "@volar/vue-language-plugin-pug"
    ]
  }
}
